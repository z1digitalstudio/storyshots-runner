  "workspaces": [
    "packages/*"
  ],
  "private": true,
  "repository": "git@github.com:z1digitalstudio/storyshots-runner.git",
  "scripts": {
    "lint": "eslint --ext .js,.jsx,.ts,.tsx packages/*/*.js packages/*/src/**/*.ts .eslintrc.js .prettierrc.js --ignore-pattern '!.*.js' --max-warnings=0",
